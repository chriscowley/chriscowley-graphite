{
  "name": "chriscowley-graphite",
  "version": "0.1.0",
  "author": "chriscowley",
  "summary": "Installs Graphite using distro packages and follow distro file system layout",
  "license": "Apache-2.0",
  "source": "https://github.com/chriscowley/chriscowley-graphite.git",
  "project_page": "https://github.com/chriscowley/chriscowley-graphite",
  "issues_url": "https://github.com/chriscowley/chriscowley-graphite/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
